    app:contentInsetStartWithNavigation="@dimen/dimen_0dp"
    app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
    app:titleMarginStart="@dimen/dimen_0dp"
    app:titleTextAppearance="@style/ActionBar.titleText" />
